{
  "variables": {
    "defaultTenantId": "d7d09513-a3f5-401c-9685-34ab6c552453",
    "adminEmail": "admin@fusionauth.io",
    "adminPassword": "password",
    "hooliAppId": "12c5424e-a44f-477b-9dab-7e12d45e5f1e",
    "piedPiperAppId": "85a03867-dccf-4882-adde-1a79aeec50df",
    "hooliClientSecret": "bYAGRDb_Jr6VxLFFSteHgsIDFfhQ6V8gTu3tnng2mTk",
    "piedPiperClientSecret": "_qsqMTIZt_T5yPb9PJqDeIAHf-hMq0587xN87XHr3lY",
    "adminUserId": "00000000-0000-0000-0000-000000000001"
  },
  "apiKeys": [
    {
      "key": "bf69486b-4733-4470-a592-f1bfce7af580",
      "description": "Standard development API key"
    }
  ],
  "requests": [
    {
      "method": "POST",
      "url": "/api/application/#{hooliAppId}",
      "tenantId": "#{defaultTenantId}",
      "body": {
        "application": {
          "name": "Hooli",
          "oauthConfiguration": {
	    "authorizedRedirectURLs": ["http://hooli.local:3001/oauth-redirect"],
	    "clientSecret": "#{hooliClientSecret}",
	    "logoutURL": "http://hooli.local:3001/endsession",
	    "enabledGrants": ["authorization_code","refresh_token"]
          },
	  "registrationConfiguration": {
            "enabled": true
          }
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/application/#{piedPiperAppId}",
      "tenantId": "#{defaultTenantId}",
      "body": {
        "application": {
          "name": "Pied Piper",
          "oauthConfiguration": {
	    "authorizedRedirectURLs": ["http://piedpiper.local:3000/oauth-redirect"],
	    "clientSecret": "#{piedPiperClientSecret}",
	    "logoutURL": "http://piedpiper.local:3000/endsession",
	    "enabledGrants": ["authorization_code","refresh_token"]
          },
	  "registrationConfiguration": {
            "enabled": true
          }
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/user/registration/#{adminUserId}",
      "body": {
        "user": {
          "email": "#{adminEmail}",
          "firstName": "Admin",
          "lastName": "User",
          "password": "#{adminPassword}",
          "imageUrl": "https://local.fusionauth.io/images/doc-profile-pictures/photo-erlich.png"
        },
        "registration": {
          "applicationId": "#{FUSIONAUTH_APPLICATION_ID}",
          "roles": [
            "admin"
          ]
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/user/registration/#{adminUserId}",
      "body": {
        "registration": {
          "applicationId": "#{hooliAppId}"
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/user/registration/#{adminUserId}",
      "body": {
        "registration": {
          "applicationId": "#{piedPiperAppId}"
        }
      }
    }
  ]
}

